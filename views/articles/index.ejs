<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <link rel="icon" href="/favicon.ico" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <meta name="theme-color" content="#000000" />
  <title>Feeds</title>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Readex+Pro%3A300%2C400%2C600%2C700"/>
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro%3A300%2C400%2C600%2C700"/>
  <link rel="stylesheet" href="/css/feeds.css"/>
</head>
<body>
<div class="top-articles-bg2">
  <div class="frame-5-kxE">
    <a href="/" class="geekyblog-SyU">GeekyBlog</a>
    <div class="group-5-8LW">
      <div class="frame-89-5We">
        <img class="search-z7p" src="/img/search-mUa.png"/>
        <input class="search-for-articles-hY2" type="text"/>
      </div>
    </div>
    <div class="frame-5-ixv">
      <!-- <a  href="/login" class="frame-6-52n">Login</a>
      <a  href="/signup" class="frame-3-XQa">Register</a> -->
    </div>
  </div>
  <div class="auto-group-xt1y-QUN">
    <div class="frame-90-vBp">
      <div class="frame-91-p2J">
        <a href="/feeds" class="feeds-tnr">Feeds</a>
        <a href="/top" class="top-Rnn">Top</a>
        <% if (''.includes(obj)) { %>
          <a href="/bookmarks" class="bookmarks-x22">Bookmarks</a>
        <% } else { %>
          <a href="/api/v1/users/bookmarks/<%= obj %>" class="bookmarks-x22">Bookmarks</a>
          <% } %> 
        <a href="/about" class="about-fx2">About </a>
        <div class="logout"></div>

      </div>
    </div>
    <div class="frame-53-ZXc">
      <div class="frame-51-HiW">
        <p class="top-articles-Rpi">All Articles</p>

        <% articles.forEach(article => { %>

        <div class="review--iSS">
          <div class="frame-4-1wL">
            <div class="frame-5-4HL">
              <!-- <div class="avatar-nj8"> -->
              <img class="avatar-nj8" src="/img/users/<%= article.user.photo %>"/>

              <!-- </div> -->
            </div>
            <% if (typeof article.user !== 'undefined' && typeof article.user.name !== 'undefined') { %>
                <div class="frame-6-4Rk"><%= article.user.name %></div>

            <% } else { %>
              <div class="frame-6-4Rk">Username</div>
            <% } %> 
            
          </div>
          <a href="articles/<%= article.slug %>" class="frame-92-hzW"><%= article.title %></a>
          <p class="item-01-10-2022-bKC"><%= article.createdAt.toLocaleDateString() %> </p>
          <p class="lorem-ipsum-dolor-sit-amet-consectetur-adipiscing-elit-nulla-varius-nisi-et-mauris-iaculis-sed-elementum-magna-suscipit-class-aptent-taciti-sociosqu-ad-litora-torquent-per-conubia-nostra-ht2"><%= article.description %></p>


          <% if (JSON.stringify(obj) === JSON.stringify(article.user['_id'])) { %>
            <div class="operations">          
              <a href="articles/edit/<%= article.id %>" class="btn btn-info">Edit</a>
  
              <form action="/articles/<%= article.id %>?_method=DELETE" method="POST" class="d-inline">
                  <button type="submit" class="btn btn-danger">Delete</button>
              </form>
          </div>

        <% } else { %>
          <div class="frame-6-4Rk"></div>
        <% } %> 

          

          
        </div>

        <% }) %>
      </div>
    </div>
  </div>
</div>

<script type="module" src="../js/feeds.js"></script>

<script src="https://cdnjs.cloudflare.com/ajax/libs/axios/1.1.2/axios.min.js"></script>


</body>